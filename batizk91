  Opened = true
  MainGUI.Visible = true
  OpenGUI.Text = ">> Close <<"
    wait(0.3)
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Jb36 Hub";
    Text = "Opened.."
})
 else
  Opened = false
  MainGUI.Visible = false
  OpenGUI.Text = ">> Open <<"
      wait(0.3)
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Jb36 Hub";
    Text = "Closed.."
})
 end
end)
 
